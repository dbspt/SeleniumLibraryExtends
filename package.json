  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dbspt/SeleniumLibraryExtends/issues"
  },
  "homepage": "https://github.com/dbspt/SeleniumLibraryExtends#readme",
  "devDependencies": {
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
